{
  "name": "zeus",
  "version": "0.1.0",
  "description": "Modular back-office of Trancendances's new website",
  "contributors": [
    {
      "name": "Brendan Abolivier",
      "email": "brendan@trancendances.fr"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trancendances/zeus.git"
  },
  "dependencies": {
    "@types/express": "4.0.35",
    "@types/node": "7.0.12",
    "@types/sequelize": "3.4.43",
    "americano": "https://github.com/babolivier/americano",
    "passport": "0.3.2",
    "pg": "6.1.5",
    "pg-native": "1.10.0",
    "revalidator": "0.3.1",
    "sequelize": "https://github.com/sequelize/sequelize",
    "typescript": "2.2.2"
  },
  "scripts": {
    "watch": "tsc -p server -w",
    "build": "tsc -p server && cp -R plugins build",
    "sync": "node ./build/server/scripts/syncdb.js",
    "sync:force": "node ./build/server/scripts/syncdb.js --force",
    "schema:update": "node ./build/server/scripts/updateschema.js"
  }
}
